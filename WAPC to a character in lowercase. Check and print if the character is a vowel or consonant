
int main() {
    char alphabet;
    printf("enter your alphabet in lowercase: ");
    scanf("%c", &alphabet);
    if(alphabet == 'a'||alphabet == 'e'||alphabet == 'i'||alphabet == 'o'||alphabet == 'u'){
        printf("alphabet %c is a vowel",alphabet);
        
    }
    else{
        printf("alphabet %c is a consonant",alphabet);
    }

    return 0;
}
