int main() {
    int num;
    printf("enter your number: ");
    scanf("%d", &num);
    if(num>0){
        if(num%2==0){
            printf("number is even");
        }
        else{
            printf("number is odd");
        }
        
    }
    else{
        printf("number is negative");
    }

    return 0;
}
