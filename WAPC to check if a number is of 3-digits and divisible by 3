int main() {
    int num;
    printf("enter number: ");
    scanf("%d",&num);
    
    if(num>=100 && num<=999){
        printf("number is a three digit number\n");
        if(num%3==0){
            printf("number is divisible by 3");
        }
        else{
            printf("number is not divisible by 3");
        }
    }
    else{
        printf("number is not three digit");
    }

    return 0;
}
